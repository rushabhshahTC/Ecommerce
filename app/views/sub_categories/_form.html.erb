<%= form_for([@category, @subcategory]) do |f| %>
    <div class="form-group">
        <%= f.label "SubCategory Name" %>
        <%= f.text_field :name %>
    </div>
    <div class="form-group">
        <%= f.submit "Create",class: 'btn btn-primary' %>
        <%= link_to 'Back', categories_path, class: 'btn btn-primary' %>
    </div>
<% end %>
